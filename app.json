{
  "expo": {
    "name": "Job",
    "slug": "Jobs",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/job.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/job.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/job.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alirezarmj.Job"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/job.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "36a23d35-6f4a-476a-876c-7cce88575ab7"
      }
    }
  }
}
